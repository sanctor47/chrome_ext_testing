{
    "manifest_version": 2,
    "name": "Test",
    "version": "1.0",
    "description":"Testing out the pipline",
    "icons":{
        "16":"./images/16.png",
        "48":"./images/48.png",
        "128":"./images/128.png"
    },
    "background":{
        "scripts": ["./background.js"]
    },
    "options_page":"./options.html",
    "browser_action":{
        "default_popup": "./popup.html"
    },
    "permissions":[
        "storage",
        "tabs",
        "https://www.google.com/*"
    ]
}